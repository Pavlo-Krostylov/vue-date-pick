{
  "name": "vue-date-pick",
  "version": "1.5.1",
  "description": "Date time picker based on Vue",
  "homepage": "https://dbrekalo.github.io/vue-date-pick/",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbrekalo/vue-date-pick"
  },
  "main": "dist/vueDatePick.js",
  "license": "MIT",
  "files": [
    "/dist",
    "/src"
  ],
  "keywords": [
    "datepicker",
    "datetimepicker",
    "date",
    "time",
    "picker",
    "pick",
    "vue"
  ],
  "scripts": {
