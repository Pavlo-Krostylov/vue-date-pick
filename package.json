  },
  "devDependencies": {
    "css-loader": "^6.7.3",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@vue/test-utils": "^2.2.10",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.7",
    "eslint": "^8.34.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
